{
  "name": "adonis-api",
  "version": "0.1",
  "adonis-version": "4.1.0",
  "description": "The API boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "node ace test",
    "nibble": "eslint-nibble --ext .js .",
    "docs": "node_modules/.bin/jsdoc --configure .jsdoc.js --verbose",
    "swagger": "swagger-jsdoc -d config/swagger.js app/Models/*.js"
  },
  "keywords": [
    "adonisjs",
    "adonis-app",
    "api",
    "boilerplate",
    "boilerplate-api"
  ],
  "author": "Sergey Lisin <sergey@abpgroup.ru>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/session": "^1.0.27",
    "@adonisjs/shield": "^1.0.8",
    "inflection": "^1.12.0",
    "mysql": "^2.16.0",
    "swagger-jsdoc": "^3.2.7"
  },
  "devDependencies": {
    "eslint": "5.12.1",
    "eslint-config-prettier": "4.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-loader": "2.1.1",
    "eslint-nibble": "5.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsdoc": "4.1.0",
    "eslint-plugin-no-loops": "0.3.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-unicorn": "7.1.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.4.0",
    "prettier": "1.16.2",
    "minami": "1.2.3",
    "jsdoc": "3.5.5"
  },
  "autoload": {
    "App": "./app"
  }
}
